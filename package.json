{
  "name": "DeFi",
  "version": "1.0.0",
  "description": "Smart Contract for Solidity",
  "main": "truffle-config.js",
  "scripts": {
    "truffle": "truffle compile",
    "migrate-rinkeby": "truffle migrate --network rinkeby --reset --f 2 -t 2",
    "verify": "truffle run verify V7Coin --network rinkeby",
    "test": "truffle test",
    "compile": "rimraf build/contracts && truffle compile",
    "test coverage": "truffle run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vodis/solidity-contracts-de-fi.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts"
  ],
  "author": "vodis [[voitenko.d]]",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.9",
    "@openzeppelin/contracts": "^4.2.0",
    "@typechain/web3-v1": "^1.0.0",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.23",
    "web3": "^1.0.0",
    "truffle-hdwallet-provider": "^1.0.17",
    "solidity-coverage": "^0.7.16",
    "typescript": "^3.7.0",
    "bn.js": "^4.11.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.16"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.4.2",
    "@typechain/ethers-v5": "^1.0.0",
    "rimraf": "^3.0.2",
    "truffle": "^5.3.6",
    "truffle-plugin-verify": "^0.5.11",
    "typechain": "^2.0.0"
  }
}
